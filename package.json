{
  "name": "gc-json-logger-express",
  "version": "0.0.0",
  "private": false,
  "license": "MIT",
  "author": "Igor Korchagin (https://github.com/igrek8)",
  "keywords": [
    "logger",
    "logging",
    "monitoring",
    "structured-logging",
    "google-cloud"
  ],
  "description": "Structured logging for express.js app in GKE",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/igrek8/gc-json-logger-express#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/igrek8/gc-json-logger-express"
  },
  "bugs": {
    "url": "https://github.com/igrek8/gc-json-logger-express/issues"
  },
  "files": [
    "./dist",
    "./runkit.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf ./dist",
    "test": "jest",
    "compile": "tsc --project ./tsconfig.build.json",
    "build": "npm run clean && npm run compile",
    "lint": "eslint \"**/*\"",
    "format": "prettier --check \"**/*\"",
    "docs:build": "typedoc --out ./docs",
    "docs:publish": "cp ./public/index.html ./docs/index.html && gh-pages --dist ./docs",
    "docs": "npm run docs:build && npm run docs:publish",
    "watch": "tsc --watch --project ./tsconfig.build.json"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^29.0.3",
    "@types/node": "^16.11.59",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.20",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.3",
    "@types/express": "^4",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.2.4",
    "@types/ms": "^0.7.31"
  },
  "dependencies": {
    "tslib": "^2",
    "ms": "^2",
    "express": "^4",
    "gc-json-logger": "^0.0.7"
  },
  "runkitExampleFilename": "./runkit.js",
  "engines": {
    "node": ">=14"
  }
}
