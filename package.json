{
  "name": "gc-json-logger-express",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Igor Korchagin (https://github.com/igrek8)",
  "keywords": [
    "logger",
    "logging",
    "monitoring",
    "structured-logging",
    "google-cloud"
  ],
  "description": "Structured logging for express.js app in GKE",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "runkitExampleFilename": "runkit.js",
  "homepage": "https://github.com/igrek8/gc-json-logger-express#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/igrek8/gc-json-logger-express"
  },
  "bugs": {
    "url": "https://github.com/igrek8/gc-json-logger-express/issues"
  },
  "files": [
    "dist",
    "runkit.js"
  ],
  "prettier": "@igrek8/prettier",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "@igrek8/semantic-release"
  },
  "jest": {
    "preset": "@igrek8/jest",
    "rootDir": "src"
  },
  "eslintConfig": {
    "extends": "@igrek8/eslint-config",
    "env": {
      "node": true
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "compile": "tsc -p tsconfig.build.json",
    "build": "npm run clean && npm run compile",
    "lint": "eslint \"**/*\"",
    "format": "prettier --check \"**/*\"",
    "docs": "typedoc build -o docs && typedoc publish -o docs"
  },
  "dependencies": {
    "tslib": "^2",
    "ms": "^2",
    "gc-json-logger": "^1"
  },
  "peerDependencies": {
    "express": "^4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@igrek8/eslint-config": "1.0.6",
    "@igrek8/jest": "1.0.1",
    "@igrek8/prettier": "1.0.1",
    "@igrek8/semantic-release": "1.0.2",
    "@igrek8/typescript": "1.0.1",
    "@igrek8/typedoc": "1.0.1",
    "@types/node": "^18.14.2",
    "husky": "^8.0.1",
    "rimraf": "^4.1.2",
    "@types/express": "^4",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.2.4",
    "@types/ms": "^0.7.31",
    "tslib": "2.5.0",
    "gc-json-logger": "1.4.0",
    "ms": "2.1.3"
  }
}
